program test{
    declare
	    sum, i, j
    enddeclare

    procedure mpla(in a, in b, inout sum){
        print(a);
        print(b);
        sum := a + b;
    }

    i := 0;
    do{
        j := 2*i;
        call mpla(in i, in j, inout sum);
        i := i +1 ;
    }while(i<j)
    print(sum);
}




